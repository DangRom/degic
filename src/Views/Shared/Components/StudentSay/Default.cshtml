@model IEnumerable<DegicEducation.Models.FeedbackViewModel>

<section class="divider parallax layer-overlay overlay-theme-colored-9" data-background-ratio="0.5" data-bg-img="http://placehold.it/1920x1280">
                <div class="container pb-50">
                    <div class="section-title">
                        <div class="row">
                            <div class="col-md-6">
                                <h2 class="mt-0 mb-0 text-uppercase line-bottom text-white font-28">CẢM NHẬN CỦA HỌC VIÊN<span class="font-30 text-theme-color-2">.</span></h2>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 mb-10">
                            <div class="owl-carousel-2col boxed" data-dots="true">
                               @foreach(var f in Model)
                               {
                                  <div class="item">
                                       <div class="testimonial pt-10">
                                          <div class="thumb pull-left mb-0 mr-0 pr-20">
                                             <img width="75" class="img-circle" alt="" src="@f.Avatar">
                                          </div>
                                          <div class="ml-100 ">
                                             <h4 class="text-white mt-0">@Html.Raw(@f.Content)</h4>
                                             <p class="author mt-20">- <span class="text-theme-color-2">@f.FullName</span> <small><em class="text-gray-lightgray"></em></small></p>
                                          </div>
                                       </div>
                                  </div>
                               }
                            </div>
                        </div>
                    </div>
                </div>
            </section>