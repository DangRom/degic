@model IEnumerable<DegicEducation.Models.CustomerViewModel>

<section class="clients bg-theme-color-2">
                <div class="container pt-10 pb-0">
                    <div class="row">
                        <div class="col-md-12">
                            <!-- Section: Clients -->
                            <div class="owl-carousel-6col clients-logo transparent text-center owl-nav-top">
                               @foreach(var c in Model)
                               {
                                  <div class="item">
                                       <a href="#"><img src="@c.Image" alt="@c.Name"></a>
                                 </div>
                               }
                            </div>
                        </div>
                    </div>
                </div>
            </section>